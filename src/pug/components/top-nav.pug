nav(class='top-nav')
	div(class='top-nav__container')
		div(class='top-nav__align')
			a(href='./', class='top-nav__link')
				img(src='./assets/img/logo.png', alt='', class='top-nav__logo')
		div
			ul(class='top-nav__menu')
				-for(var i = 0; i < menu.length ; i++)		
					-var menus = menu[i]
						li(class='top-nav__li'): a(href= menus.url class='top-nav__anchor')=menus.menuItem
		div(class='top-nav__buttons')
			button(class="btn btn__login") REGISTRATE
			button(class="btn btn__register") INICIAR SESIÃ“N
			button(type='button', class='top-nav__mb-iconMenu hamburger hamburger--slider-r')
						span.hamburger-box
							span.hamburger-inner




			
